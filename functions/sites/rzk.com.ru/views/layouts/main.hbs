<!doctype html>
  <html lang="{{#if (equals envSite.data.lang "ru-ua")}}ru{{else}}{{envSite.lang}}{{/if}}">
    <head>
      <meta charset="utf-8">
      <meta name="viewport" content="width=device-width, initial-scale=1">
      <script type="module" crossorigin src="/assets/index-07745b19.js"></script>
      <link rel="stylesheet" href="/assets/index-5b326555.css">
      <title>{{#if title}}{{title}} | {{envSite.i18n.siteName}}{{else}}{{envSite.i18n.siteTitle}}{{/if}}</title>
      <link rel="icon" href="/favicon-{{envSite.lang}}.ico">
      <link rel="icon" href="/logo/logo-{{envSite.lang}}.svg" type="image/svg+xml">
      <meta name="description" content="{{#if description}}{{description}}{{else}}{{envSite.i18n.siteDesc}}{{/if}}">
      <!-- Google Tag Manager -->
      <script>(function(w,d,s,l,i){w[l]=w[l]||[];w[l].push({'gtm.start':
      new Date().getTime(),event:'gtm.js'});var f=d.getElementsByTagName(s)[0],
      j=d.createElement(s),dl=l!='dataLayer'?'&l='+l:'';j.async=true;j.src=
      'https://www.googletagmanager.com/gtm.js?id='+i+dl;f.parentNode.insertBefore(j,f);
      })(window,document,'script','dataLayer','{{envSite.gtag}}');</script>
      <!-- End Google Tag Manager -->
      {{#if (equals envSite.lang "ru")}}
      <!-- Yandex.Metrika counter --> <script type="text/javascript" > (function(m,e,t,r,i,k,a){m[i]=m[i]||function(){(m[i].a=m[i].a||[]).push(arguments)}; m[i].l=1*new Date(); for (var j = 0; j < document.scripts.length; j++) {if (document.scripts[j].src === r) { return; }} k=e.createElement(t),a=e.getElementsByTagName(t)[0],k.async=1,k.src=r,a.parentNode.insertBefore(k,a)}) (window, document, "script", "https://mc.yandex.ru/metrika/tag.js", "ym"); ym(50984510, "init", { clickmap:true, trackLinks:true, accurateTrackBounce:true, webvisor:true, trackHash:true }); </script> <noscript><div><img src="https://mc.yandex.ru/watch/50984510" style="position:absolute; left:-9999px;" alt="" /></div></noscript> <!-- /Yandex.Metrika counter -->
      {{else}}
      <link rel="alternate" hreflang="ru-UA" href="{{envSite.data.urlRu}}">
      <link rel="alternate" hreflang="uk-UA" href="{{envSite.data.urlUk}}">  
      {{/if}}
      {{#if currentCatalog}}
        <script type="application/ld+json">
          {
            "@context": "https://schema.org",
            "@type": "BreadcrumbList",
            "itemListElement": [
          {{#each currentCatalog.pathArray as |item|}}
            {
              "@type": "ListItem",
              "position": {{@index }},
            {{#if (not item.name ../currentCatalog.name)}}
              "name": "{{item.name}}",
              "item": "{{../envSite.domain}}/c/{{item.url}}"
            },
            {{else}}
              "name": "{{item.name}}"
            }
            {{/if}}
          {{/each}}
            ]
          }
        </script>  
      {{/if}}
      {{#if product}}
      <script type="application/ld+json">
        {
          "@context": "https://schema.org/",
          "@type": "Product",
          "name": "{{#if product.brand}}{{product.brand}} {{/if}}{{product.name}}",
          "mpn": "{{product.id}}",
          "image": [
            "{{product.img1}}",
            "{{product.img2}}"
          ],
          "description": "{{envSite.i18n.siteDesc}}",
        {{#if product.brand}}
          "brand": {
            "@type": "Brand",
            "name": "{{product.brand}}"
          },
        {{/if}}
          "offers": {
            "@type": "Offer",
            "url": "{{product.url}}",
            "itemCondition": "https://schema.org/NewCondition",
            "availability": "https://schema.org/InStock",
            "price": {{product.price}},
            "priceCurrency": "{{envSite.priceCurrency}}"
          }
        }
      </script>
      {{/if}}
    </head>
    <body>
      <!-- Google Tag Manager (noscript) -->
      <noscript><iframe src="https://www.googletagmanager.com/ns.html?id={{envSite.gtag}}"
      height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
      <!-- End Google Tag Manager (noscript) -->
      <nav class="navbar navbar-expand navbar-dark bg-{{#if (equals envSite.data.lang "uk")}}dark{{else}}secondary{{/if}}">
        <div class="container-xl">
          <button class="navbar-toggler d-inline" type="button" data-bs-toggle="offcanvas" data-bs-target="#offcanvasNavbar" aria-controls="offcanvasNavbar">
            <span class="navbar-toggler-icon"></span>
          </button>
          <a class="navbar-brand mx-2" href="/{{#if (equals envSite.data.lang "ru-ua")}}ru{{/if}}">
            <img src="/logo/logo-{{envSite.lang}}.svg" alt="{{envSite.i18n.siteName}}" height="40" width="40">
            <span class="d-none d-md-inline h4 align-middle">{{envSite.i18n.siteName}}</span>
          </a>
          <ul class="navbar-nav me-auto mb-2 mb-lg-0">
            <li class="nav-item">
              {{#if (equals envSite.data.lang "ru-ua")}}
                <a class="nav-link" aria-current="page" href="/">UA</a>
              {{/if}}
            </li>
          </ul>
          <div id="autocomplete" class="w-100 overflow-hidden"></div>
          <ul class="navbar-nav me-2">
            <li class="nav-item"></li>
              <a class="text-nowrap text-white nav-link{{#if cart}} active{{/if}} position-relative" href="{{#if (equals envSite.data.lang "ru-ua")}}/ru{{/if}}/cart">
                <i class="bi bi-cart3"></i> <strong>{{envSite.i18n.aCart}}</strong>
                <span id="cartCountNav" class="{{#if (equals cartInfo.cartCount 0)}}d-none {{/if}}position-absolute top-0 start-100 translate-middle badge rounded-pill bg-success mt-1">{{cartInfo.cartCount}}</span>
              </a>
            </li>
          </ul>
        </div>
      </nav>
      <div class="offcanvas offcanvas-start" tabindex="-1" id="offcanvasNavbar" aria-labelledby="offcanvasNavbarLabel">
        <div class="offcanvas-header">
          <h5 class="offcanvas-title" id="offcanvasNavbarLabel">{{envSite.i18n.siteName}}</h5>
          <button type="button" class="btn-close text-reset" data-bs-dismiss="offcanvas" aria-label="Close"></button>
        </div>
        <div class="offcanvas-body">
          <p class="lead">{{{envSite.i18n.desc}}}</p>
          <nav class="nav flex-column">
            {{#if envSite.user.auth}}
              <a class="nav-link" href="{{#if (equals envSite.data.lang "ru-ua")}}/ru{{/if}}/login"><i class="bi bi-person-fill"></i> {{envSite.user.firstName}}</a>
            {{else}}
              <a class="nav-link" href="{{#if (equals envSite.data.lang "ru-ua")}}/ru{{/if}}/login"><i class="bi bi-person-fill"></i> {{envSite.i18n.aLogin}}</a>
            {{/if}}
            {{#each envSite.i18n.phones as |phone|}}
              <a class="nav-link" href="tel:{{phone}}"><i class="bi bi-telephone-fill"></i> {{phone}}</a>
            {{/each}}
            <a class="nav-link" href="//t.me/{{envSite.botName}}" target="_blank"><i class="bi bi-telegram"></i> Telegram бот @{{envSite.botName}}</a>
            <a class="nav-link" href="//t.me/{{envSite.chatName}}" target="_blank"><i class="bi bi-telegram"></i> Telegram чат @{{envSite.chatName}}</a>
            {{#if (equals envSite.data.lang "ru")}}
            <a class="nav-link" href="//t.me/rzkmarket" target="_blank"><i class="bi bi-telegram"></i> Telegram канал @RzkMarket</a>
            <a class="nav-link" href="//www.instagram.com/rzkmarket" target="_blank"><i class="bi bi-instagram"></i> Instagram @rzkmarket</a>
            {{/if}}
            <a class="nav-link" href="mailto:{{envSite.email}}"><i class="bi bi-envelope-fill"></i> {{envSite.email}}</a>
            <a class="nav-link" href="{{#if (equals envSite.data.lang "ru-ua")}}/ru{{/if}}/news">{{envSite.i18n.tNews}}</a>
            <a class="nav-link" href="{{#if (equals envSite.data.lang "ru-ua")}}/ru{{/if}}/delivery-info">{{envSite.i18n.aDelivery}}</a>
            <a class="nav-link" href="{{#if (equals envSite.data.lang "ru-ua")}}/ru{{/if}}/return-policy">{{envSite.i18n.aReturn}}</a>
            {{#if (equals envSite.lang "uk")}}
              <a class="nav-link" href="//gunsan.com.ua" target="_blank">Gunsan.com.ua</a>
              <a class="nav-link" href="//nilson.com.ua" target="_blank">Nilson.com.ua</a>
            {{/if}}
            <a class="nav-link disabled" href="#">{{envSite.i18n.siteName}} © {{year}}</a>
            </nav>
        </div>
      </div>
      <div class="container-xl d-none mt-3 mb-5" id="search">
        <div class="d-grid gap-2">
          <button class="btn btn-success d-block d-lg-none mb-3" type="button" data-bs-toggle="offcanvas" data-bs-target="#offcanvasResponsive" aria-controls="offcanvasResponsive">
            {{envSite.i18n.btnFilter}}
          </button>
        </div>
        <div class="row">
          <div class="col fs-3 mb-3 d-none" id="stats"></div>
        </div>
        <div class="row">
          <div class="col-lg-3">
            <div class="offcanvas-lg offcanvas-start" tabindex="-1" id="offcanvasResponsive" aria-labelledby="offcanvasResponsiveLabel">
              <div class="offcanvas-header">
                <h5 class="offcanvas-title" id="offcanvasResponsiveLabel">{{envSite.i18n.btnFilter}}</h5>
                <button type="button" class="btn-close" data-bs-dismiss="offcanvas" data-bs-target="#offcanvasResponsive" aria-label="Close"></button>
              </div>
              <div class="offcanvas-body flex-column">
                <div class="accordion accordion-flush" id="accordionPanelsStayOpenExample">
                  <div class="accordion-item">
                    <h2 class="accordion-header" id="panelsStayOpen-headingOne">
                      <button class="accordion-button" type="button" data-bs-toggle="collapse" data-bs-target="#panelsStayOpen-collapseOne" aria-expanded="true" aria-controls="panelsStayOpen-collapseOne">
                        {{envSite.i18n.aCatGoods}}
                      </button>
                    </h2>
                    <div id="panelsStayOpen-collapseOne" class="accordion-collapse collapse show" aria-labelledby="panelsStayOpen-headingOne">
                      <div class="accordion-body p-0">
                        <div id="hierarchical-menu" class="list-group list-group-flush py-2"></div>
                      </div>
                    </div>
                  </div>
                  <div class="accordion-item">
                    <h2 class="accordion-header" id="panelsStayOpen-heading3">
                      <button class="accordion-button" type="button" data-bs-toggle="collapse" data-bs-target="#panelsStayOpen-collapse3" aria-expanded="true" aria-controls="panelsStayOpen-collapse3">
                        {{envSite.i18n.tCategory}}
                      </button>
                    </h2>
                    <div id="panelsStayOpen-collapse3" class="accordion-collapse collapse show" aria-labelledby="panelsStayOpen-heading3">
                      <div class="accordion-body p-0">
                        <div id="refinement-list-subcategory" class="list-group py-2"></div>
                      </div>
                    </div>
                  </div>
                  <div class="accordion-item">
                    <h2 class="accordion-header" id="panelsStayOpen-headingTwo">
                      <button class="accordion-button" type="button" data-bs-toggle="collapse" data-bs-target="#panelsStayOpen-collapseTwo" aria-expanded="true" aria-controls="panelsStayOpen-collapseTwo">
                        Бренд
                      </button>
                    </h2>
                    <div id="panelsStayOpen-collapseTwo" class="accordion-collapse collapse show" aria-labelledby="panelsStayOpen-headingTwo">
                      <div class="accordion-body p-0">
                        <div id="refinement-list-brand" class="list-group py-2"></div>
                      </div>
                    </div>
                  </div>
                  <div class="accordion-item">
                    <h2 class="accordion-header" id="panelsStayOpen-heading4">
                      <button class="accordion-button" type="button" data-bs-toggle="collapse" data-bs-target="#panelsStayOpen-collapse4" aria-expanded="true" aria-controls="panelsStayOpen-collapse4">
                        {{envSite.i18n.tSeller}}
                      </button>
                    </h2>
                    <div id="panelsStayOpen-collapse4" class="accordion-collapse collapse show" aria-labelledby="panelsStayOpen-heading4">
                      <div class="accordion-body p-0">
                        <div id="refinement-list-seller" class="list-group py-2"></div>
                      </div>
                    </div>
                  </div>
                </div>
                <div id="powered-by" class="pt-2"></div>
              </div>
            </div>
          </div>
          <div class="col-md-12 col-lg-9">
            <h3 id="cat_header" class="mb-3 d-block d-lg-none d-none"></h3>
            <ul id="current-refinements" class="nav mb-3 d-none"></ul>
            <div id="hits" class="row row-cols-2 row-cols-sm-3 row-cols-md-4 row-cols-lg-5 g-2"></div>
            <div id="loading-indicator">
              <div class="d-flex justify-content-center mt-3">
                <div class="spinner-border" role="status">
                  <span class="visually-hidden">Loading...</span>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="container-xl mt-3 mb-5"  id="main">
        {{{body}}}
      </div>
      <!-- Modal show product -->
      <div class="modal fade" id="productModal" tabindex="-1" aria-labelledby="productModalLabel" aria-hidden="true">
        <div class="modal-dialog">
          <div class="modal-content">
            <div class="modal-header">
              <h6 class="modal-title" id="productModalLabel">{{envSite.i18n.modalShowTitle}}</h6>
              <a id="fullscreen" href="#" class="ms-2"><i class="bi bi-arrows-fullscreen"></i></a>
              <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
            </div>
          </div>
        </div>
      </div>
      <!-- Modal cart -->
      <div class="modal fade" id="cartAddModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog">
          <div class="modal-content">
            <div class="modal-header">
              <h5 class="modal-title" id="exampleModalLabel">Modal title</h5>
              <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <form id="addToCartForm">
              <div class="modal-body">
                <div class="input-group mb-3">
                  <input type="number" min="0" id="qty" name="qty" class="form-control" placeholder="{{envSite.i18n.placeholderQty}}" aria-label="{{envSite.i18n.placeholderQty}}" aria-describedby="basic-addon2">
                  <span class="input-group-text" id="basic-addon2"></span>
                </div>
              </div>
              <div class="modal-footer">
                <button type="button" id="deleteFromCart" class="btn btn-danger">{{envSite.i18n.btnDel}}</button>
                <button type="submit" id="addToCart" class="btn btn-success" data-lang="{{envSite.lang}}" data-dev-prefix="{{envSite.devPrefix}}">{{envSite.i18n.btnBuy}}</button>
              </div>
            </form>
          </div>
        </div>
      </div>
      <!-- Toast -->
      <div class="toast-container position-fixed bottom-0 end-0 p-3">
        <div id="liveToast" class="toast text-bg-light" data-bs-delay="3000" role="alert" aria-live="assertive" aria-atomic="true">
          <div class="toast-header">
            <img src="/logo/logo-{{envSite.lang}}.svg" width="" height="30" class="me-2" alt="...">
            <strong class="me-auto" id="toast-seller">RZK Market UA</strong>
            <button type="button" class="btn-close" data-bs-dismiss="toast" aria-label="Close"></button>
          </div>
          <div class="toast-body">
          </div>
        </div>
      </div>
    </body>
  </html>